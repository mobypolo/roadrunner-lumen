{
  "name": "mobypolo/roadrunner-lumen",
  "description": "RoadRunner <-> Lumen Integration Bridge",
  "type": "library",
  "license": "MIT",
  "version": "1.1",
  "keywords": [
    "lumen",
    "roadrunner",
    "php 8",
    "lumen 8"
  ],
  "authors": [
    {
      "email": "to@mobypolo.com",
      "name": "Moby Polo"
    }
  ],
  "require": {
    "laravel/lumen-framework": "~8.0",
    "spiral/roadrunner": "^2.0.0",
    "spiral/goridge": "^3.0",
    "symfony/psr-http-message-bridge": "^1.2 || ^2.0",
    "nyholm/psr7": "^1.1"
  },
  "require-dev": {
    "phpunit/phpunit": "~8.3"
  },
  "autoload": {
    "psr-4": {
      "mobypolo\\RoadRunnerLumen\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "classmap": [
      "tests/"
    ]
  },
  "bin": [
    "bin/rr-lumen-worker"
  ],
  "prefer-stable": true
}
