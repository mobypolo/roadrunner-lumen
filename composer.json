{
  "name": "romannovo/roadrunner-lumen",
  "description": "RoadRunner <-> Lumen Integration Bridge",
  "type": "library",
  "license": "MIT",
  "version": "1.1.2",
  "keywords": [
    "lumen",
    "roadrunner",
    "php 8",
    "lumen 8"
  ],
  "authors": [
    {
      "email": "roman.novo.95@gmail.com",
      "name": "Roman Novo"
    }
  ],
  "require": {
    "laravel/lumen-framework": "^8",
    "spiral/roadrunner": "^2.0.0",
    "spiral/goridge": "^3.0",
    "symfony/psr-http-message-bridge": "^1.2 || ^2.0",
    "nyholm/psr7": "^1.1"
  },
  "require-dev": {
    "phpunit/phpunit": "~8.3"
  },
  "autoload": {
    "psr-4": {
      "mobypolo\\RoadRunnerLumen\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "classmap": [
      "tests/"
    ]
  },
  "bin": [
    "bin/rr-lumen-worker"
  ],
  "prefer-stable": true
}
